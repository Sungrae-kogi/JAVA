package backJoon;

import java.util.Scanner;


public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		int[][] num = new int[n+1][n+1];
		
		for(int i=1; i<=n; i++) {
			for(int j=1; j<=i; j++) {
				if(i>=j) {
					num[i][j]=sc.nextInt();
				}else {
					num[i][j]=0;
				}
			}
		}
		
		for(int i=1; i<=n; i++) {
			for(int j=1; j<=i; j++) {
				System.out.printf(num[i][j]+" ");
			}
			System.out.println();
		}
		

		int k=0;
		int p=0;
		int sum = num[1][1];
		System.out.println(sum);
		
		for(int i=1; i<=n; i++) {
			for(int j=1; j<=i; j++) {
				
			}
		}
		System.out.println(sum);
	}	
}